{
  "GLOBAL": {
    "RUN_TYPE": "GEO_OPT"
  },
  "MOTION": {
    "GEO_OPT": {
      "TYPE": "minimization",
      "OPTIMIZER": "BFGS",
      "MAX_ITER": 200
    },
    "MD": {
      "THERMOSTAT": {
        "NOSE": {
          "LENGTH": 3,
          "YOSHIDA": 3,
          "TIMECON": 1000,
          "MTS": 2
        }
      },
      "ENSEMBLE": "NVT",
      "STEPS": 2000000,
      "TIMESTEP": 0.5,
      "TEMPERATURE": 330.0,
      "TEMP_TOL": 50
    },
    "PRINT": {
      "TRAJECTORY": {
        "EACH": {
          "MD": 1
        }
      },
      "VELOCITIES": {
        "EACH": {
          "MD": 1
        }
      },
      "FORCES": {
        "_": "OFF"
      },
      "RESTART_HISTORY": {
        "EACH": {
          "MD": 2000
        }
      },
      "RESTART": {
        "BACKUP_COPIES": 1,
        "EACH": {
          "MD": 1
        }
      }
    }
  },
  "FORCE_EVAL": {
    "METHOD": "Quickstep",
    "DFT": {
      "BASIS_SET_FILE_NAME": "BASIS_MOLOPT",
      "POTENTIAL_FILE_NAME": "GTH_POTENTIALS",
      "QS": {
        "EPS_DEFAULT": 1e-13,
        "EXTRAPOLATION": "ASPC",
        "EXTRAPOLATION_ORDER": 2
      },
      "SCF": {
        "SCF_GUESS": "RESTART",
        "EPS_SCF": 3e-7,
        "MAX_SCF": 50,
        "OUTER_SCF": {
          "EPS_SCF": 3e-7,
          "MAX_SCF": 10
        },
        "OT": {
          "MINIMIZER": "DIIS",
          "PRECONDITIONER": "FULL_SINGLE_INVERSE",
          "ENERGY_GAP": 0.1
        },
        "XC": {
          "XC_FUNCTIONAL": {
            "_": "PBE"
          },
          "VDW_POTENTIAL": {
            "DISPERSION_FUNCTIONAL": "PAIR_POTENTIAL",
            "PAIR_POTENTIAL": {
              "TYPE": "DFTD3",
              "PARAMETER_FILE_NAME": "dftd3.dat",
              "REFERENCE_FUNCTIONAL": "PBE"
            }
          }
        },
        "PRINT": {
          "E_DENSITY_CUBE": {
            "ADD_LAST": "NUMERIC",
            "EACH": {
              "GEP_OPT": 0
            }
          },
          "PDOS": {
            "COMPONENTS": null,
            "NLUMO": -1,
            "ADD_LAST": "NUMERIC",
            "EACH": {
              "MD": 50,
              "GEO_OPT": 0
            },
            "COMMON_ITERATION_LEVELS": 0,
            "APPEND": "T"
          },
          "MO_CUBES": {
            "NHOMO": 10,
            "NLUMO": 10,
            "WRITE_CUBE": "F",
            "ADD_LAST": "NUMERIC",
            "EACH": {
              "MD": 50,
              "GEO_OPT": 0
            }
          },
          "V_HARTREE_CUBE": {
            "_": "ON",
            "APPEND": "T",
            "EACH": {
              "MD": 50,
              "GEO_OPT": 0
            },
            "ADD_LAST": "NUMERIC"
          }
        }
      },
      "PRINT": {
        "STRESS_TENSOR": {
          "_": null
        }
      }
    },
    "SUBSYS": {
      "KIND": []
    }
  }
}
